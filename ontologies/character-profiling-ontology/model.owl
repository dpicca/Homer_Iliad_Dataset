        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/ma-ont#title -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/ns/ma-ont#title"/>
    


